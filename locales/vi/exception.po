msgstr "Trang web tạm thời không khả dụng."

# in Dotclear\Exception\ExceptionEnum
msgid "Bad Request"
msgstr "Yêu cầu không hợp lệ"

# in Dotclear\Exception\ExceptionEnum
msgid "Blog handling error"
msgstr "Lỗi xử lý blog"

# in Dotclear\Exception\ExceptionEnum
msgid "Application configuration error"
msgstr "Lỗi cấu hình ứng dụng"

# in Dotclear\Exception\ExceptionEnum
msgid "Conflict"
msgstr "Xung đột"

# in Dotclear\Exception\ExceptionEnum
msgid "Application context error"
msgstr "Lỗi bối cảnh ứng dụng"

# in Dotclear\Exception\ExceptionEnum
msgid "Database connection error"
msgstr "Lỗi kết nối cơ sở dữ liệu"

# in Dotclear\Exception\ExceptionEnum
msgid "Not Found"
msgstr "Không tìm thấy"

# in Dotclear\Exception\ExceptionEnum
msgid "Precondition Failed"
msgstr "Điều kiện tiên quyết không thành công"

