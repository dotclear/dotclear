msgstr "Dålig begäran"

# in Dotclear\Exception\ExceptionEnum
msgid "Blog handling error"
msgstr "Fel i hanteringen av bloggar"

# in Dotclear\Exception\ExceptionEnum
msgid "Application configuration error"
msgstr "Fel i applikationskonfigurationen"

# in Dotclear\Exception\ExceptionEnum
msgid "Conflict"
msgstr "Konflikt"

# in Dotclear\Exception\ExceptionEnum
msgid "Application context error"
msgstr "Fel i applikationskontext"

# in Dotclear\Exception\ExceptionEnum
msgid "Database connection error"
msgstr "Fel vid anslutning till databas"

# in Dotclear\Exception\ExceptionEnum
